{
	"manifest_version": 2,

	"name": "UOIT Professor Info",
	"description": "Injects information on UOIT professors into any UOIT domain, including 'ratemyprofessor' ratings, salary information from the sunshine list, and more. Look for the magnifying glass beside a UOIT Professor's name.",
	"version": "1.0",

	"content_scripts": [
	{
		"matches": ["http://www.science.uoit.ca/people*", 
		"http://www.businessandit.uoit.ca/people*", 
		"http://education.uoit.ca/people*", 
		"http://nuclear.uoit.ca/people*", 
		"http://www.engineering.uoit.ca/people*", 
		"http://www.healthsciences.uoit.ca/people*", 
		"http://www.socialscienceandhumanities.uoit.ca/people*",
		"https://ssbp.mycampus.ca/prod/bwskfshd.P_CrseSchdDetl",
		"https://ssbp.mycampus.ca/prod/bwskfcls.P_GetCrse",
		"http://ssbp.mycampus.ca/prod/bwckschd.p_get_crse_unsec"],
		"css": ["lib/jquery-ui.css"],
		"js": ["lib/jquery.js", "lib/jquery-ui.js", "PROF_SUNSHINE.js", "PROF_MASTERLIST.js", "transparentLogic.js"],
		"run-at": "document_start",
		"all_frames": true,
		"match_about_blank": true
	}
	],

	"web_accessible_resources": ["lib/glass.png", 
	"lib/cold-icon.png", 
	"lib/warm-icon.png", 
	"lib/steamy-icon.png",
	"lib/scorching-icon.png"],

	"icons": {"48": "lib/icon48.png",
			  "128": "lib/icon128.png"}
	
}
