{
	"manifest_version": 2,

	"name": "transparentUOIT",
	"description": "Injects information on UOIT professors into any UOIT domain, including 'ratemyprofessor' ratings, salary information from the sunshine list, and more.",
	"version": "1.0",

	"content_scripts": [
	{
		"matches": ["http://www.science.uoit.ca/*", "http://www.businessandit.uoit.ca/*", "http://education.uoit.ca/*", "http://nuclear.uoit.ca/*", "http://www.engineering.uoit.ca/*", "http://www.healthsciences.uoit.ca/*", "http://www.socialscienceandhumanities.uoit.ca/*", "http://gradstudies.uoit.ca/*"],
		"js": ["jquery.js", "PROF_MASTERLIST.js", "transparentLogic.js"],
		"run-at": "document_start"
	}
	]

	
}
